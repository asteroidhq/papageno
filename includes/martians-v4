function net_martian()
prefix set martians;
{
	martians = [ 
    169.254.0.0/16+,	# IPv4 Link-Local, RFC3927 
    172.16.0.0/12+,	# IPv4 Private, RFC1918 
    192.168.0.0/16+,	# IPv4 Private, RFC1918 
    10.0.0.0/8+,	# IPv4 Private, RFC1918
    224.0.0.0/4+, 	# IPv4 Multicast, RFC1700
    240.0.0.0/4+, 	# IPv4 Reserved, RFC1700
    0.0.0.0/32- 	# IPv4 reserved, RFC1700
	];
	if ( net ~ martians ) then {
		print "REJECTING: ",net.ip,"/",net.len," received from ",from,": Prefix is a martian";
		bgp_large_community.add((myasn,prefix_filtered,filtered_martian));
		return true;
	}
	# Any prefixes larger than /8
	if ( net ~ [ 0.0.0.0/0{0,7} ] ) then {
		bgp_large_community.add((myasn,prefix_filtered,filtered_too_short));
		return true;
	}
	if ( net ~ [ 0.0.0.0/0{25,32} ] ) then {
		bgp_large_community.add((myasn,prefix_filtered,filtered_too_long));
		return true;
	}
	return false;
}
